{
    "name": "hopeless-bot",
    "version": "2.1.2",
    "description": "A discord bot made for a private server.",
    "main": "src/index.js",
    "_moduleAliases": {
        "@root": ".",
        "@src": "src/",
        "@handlers": "src/handlers/",
        "@lib": "src/lib/",
        "@helpers": "src/helpers/",
        "@schemas": "src/database/schemas/",
        "@commands": "src/commands/",
        "@assets": "src/assets/",
        "@events": "src/events/"
    },
    "engines": {
        "node": ">=22 || >=20"
    },
    "scripts": {
        "test": "echo \"Error: no test specified\" && exit 1",
        "dev": "nodemon .",
        "trace": "node --trace-warnings .",
        "start": "node .",
        "destroyGlobal": "node scripts/destroyGlobal.js",
        "destroyGuild": "node scripts/destroyGuild.js",
        "lavalink": "cd ./lavalink && java -jar Lavalink.jar",
        "docker-up-lavalink": "cd ./lavalink && docker compose up -d",
        "docker-kill-lavalink": "docker kill lavalink"
    },
    "keywords": [
        "bot",
        "discord",
        "discord-bot",
        "javascript",
        "nodejs"
    ],
    "author": {
        "name": "theassassin0128",
        "url": "https://github.com/theassassin0128/"
    },
    "license": "MIT",
    "devDependencies": {
        "axios": "^1.7.7",
        "express": "^4.21.0",
        "nodemon": "^3.1.4"
    },
    "dependencies": {
        "@discordjs/voice": "^0.17.0",
        "ascii-table": "^0.0.9",
        "boxen": "^8.0.1",
        "colors": "^1.4.0",
        "discord-arts": "^0.6.1",
        "discord.js": "^14.15.3",
        "dotenv": "^16.4.5",
        "glob": "^11.0.0",
        "lavalink-client": "^2.4.0",
        "luxon": "^3.5.0",
        "module-alias": "^2.2.3",
        "mongoose": "^8.5.2",
        "ms": "^2.1.3",
        "pretty-ms": "^9.1.0",
        "readline": "^1.3.0"
    },
    "optionalDependencies": {
        "bufferutil": "^4.0.8",
        "chalk": "^5.3.0",
        "fixedsize-map": "^1.1.0",
        "table": "^6.8.2",
        "utf-8-validate": "^6.0.4",
        "zlib-sync": "^0.1.9"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/theassassin0128/Hopeless-Bot.git"
    },
    "bugs": {
        "url": "https://github.com/theassassin0128/Hopeless-Bot/issues"
    },
    "homepage": "https://github.com/theassassin0128/Hopeless-Bot#readme"
}
